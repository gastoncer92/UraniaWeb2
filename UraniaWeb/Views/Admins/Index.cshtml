@model IEnumerable<UraniaWeb.Models.Admin>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Crear nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NameAdmin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PassAdmin)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NameAdmin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PassAdmin)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAdmin">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAdmin">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAdmin">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
